{
  "name": "@ecommerce/cache",
  "version": "0.1.0",
  "description": "Caching layer infrastructure with Redis support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "redis": "^4.6.13",
    "ioredis": "^5.3.2",
    "node-cache": "^5.1.2",
    "lru-cache": "^10.2.0",
    "compression": "^1.7.4",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@types/compression": "^1.7.5",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.1.2",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "rimraf": "^5.0.5"
  },
  "peerDependencies": {
    "@ecommerce/shared": "workspace:*",
    "@ecommerce/config": "workspace:*"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./providers": {
      "import": "./dist/providers/index.js",
      "require": "./dist/providers/index.js",
      "types": "./dist/providers/index.d.ts"
    },
    "./strategies": {
      "import": "./dist/strategies/index.js",
      "require": "./dist/strategies/index.js",
      "types": "./dist/strategies/index.d.ts"
    },
    "./middleware": {
      "import": "./dist/middleware/index.js",
      "require": "./dist/middleware/index.js",
      "types": "./dist/middleware/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "cache",
    "redis",
    "caching",
    "performance",
    "ecommerce"
  ],
  "author": "E-commerce Team",
  "license": "MIT"
}