{
  "passwordPolicy": {
    "minLength": 12,
    "maxLength": 128,
    "requireUppercase": true,
    "requireLowercase": true,
    "requireNumbers": true,
    "requireSpecialChars": true,
    "specialChars": "!@#$%^&*()_+-=[]{}|;:,.<>?",
    "preventCommonPasswords": true,
    "preventUserInfoInPassword": true,
    "preventRepeatingChars": true,
    "maxRepeatingChars": 3,
    "preventSequentialChars": true,
    "maxSequentialChars": 3,
    "passwordHistory": 12,
    "maxAge": 90,
    "minAge": 1,
    "lockoutThreshold": 5,
    "lockoutDuration": 30,
    "warningDays": 14
  },
  "commonPasswords": [
    "password",
    "123456",
    "123456789",
    "12345678",
    "12345",
    "1234567",
    "admin",
    "password123",
    "qwerty",
    "abc123",
    "Password1",
    "welcome",
    "login",
    "master",
    "hello",
    "guest",
    "root",
    "administrator"
  ],
  "validation": {
    "checkBreachedPasswords": true,
    "breachCheckService": "haveibeenpwned",
    "allowCompromisedPasswords": false,
    "strengthMeter": true,
    "realTimeValidation": true
  },
  "enforcement": {
    "enforceOnRegistration": true,
    "enforceOnPasswordChange": true,
    "enforceOnPasswordReset": true,
    "allowTemporaryPasswords": false,
    "temporaryPasswordExpiry": 24
  }
}