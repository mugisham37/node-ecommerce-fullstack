{
  "name": "@ecommerce/mobile",
  "version": "1.0.0",
  "description": "React Native mobile application for e-commerce inventory management",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "dev": "react-native start",
    "build": "react-native build-android --mode=release",
    "build:ios": "react-native build-ios --mode=Release",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "clean": "react-native clean && rm -rf node_modules",
    "clean:android": "cd android && ./gradlew clean",
    "clean:ios": "cd ios && xcodebuild clean",
    "pod-install": "cd ios && pod install",
    "reset-cache": "react-native start --reset-cache"
  },
  "dependencies": {
    "@ecommerce/api-client": "workspace:*",
    "@ecommerce/shared": "workspace:*",
    "@ecommerce/validation": "workspace:*",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/drawer": "^6.6.6",
    "@trpc/client": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@tanstack/react-query": "^4.36.1",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-reanimated": "^3.6.2",
    "react-native-vector-icons": "^10.0.3",
    "react-native-svg": "^14.1.0",
    "react-native-async-storage": "^1.21.0",
    "react-native-keychain": "^8.2.0",
    "react-native-device-info": "^10.13.0",
    "react-native-permissions": "^4.1.5",
    "react-native-camera": "^4.2.1",
    "react-native-vision-camera": "^3.6.17",
    "react-native-barcode-mask": "^1.2.4",
    "react-native-qrcode-scanner": "^1.5.5",
    "react-native-image-picker": "^7.1.0",
    "react-native-document-picker": "^9.1.1",
    "react-native-fs": "^2.20.0",
    "react-native-share": "^10.0.2",
    "react-native-toast-message": "^2.2.0",
    "react-native-modal": "^13.0.1",
    "react-native-paper": "^5.12.3",
    "react-native-elements": "^3.4.3",
    "zustand": "^4.4.7",
    "superjson": "^2.2.1",
    "date-fns": "^3.2.0",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.73.1",
    "@react-native/metro-config": "^0.73.2",
    "@react-native/typescript-config": "^0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "react-native",
    "mobile",
    "ecommerce",
    "inventory",
    "typescript"
  ],
